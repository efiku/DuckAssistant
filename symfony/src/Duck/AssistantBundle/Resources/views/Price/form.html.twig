{% extends 'DuckAssistantBundle::base.html.twig' %}

{% block title %}Dodaj Cene{% endblock %}

{% block body %}
<div class="container">
<div class="row">
    <div class="form-group">

            {{ form_start(form) }}
            {{ form_errors(form) }}
            {{ form_row(form.price,{ 'attr': {'class': 'form-control'}}) }}
            {{ form_row(form.product,{ 'attr': {'class': 'form-control'}}) }}
            {{ form_row(form.shop,{ 'attr': {'class': 'form-control'}}) }}
        <br>
        <button type="submit" class="form-control btn btn-success">Zapisz</button>
            {{ form_end(form) }}
    </div>

</div>
</div>
{% endblock %}
